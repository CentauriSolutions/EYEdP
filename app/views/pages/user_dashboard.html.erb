<h1 class="flex justify-center">Welcome, <%= current_user.username || current_user.email %></h1>

<%= @template.render.html_safe %>
<%= will_paginate @logins %>

<table class="table table-striped">
  <tbody>
    <%- @logins.each do |login| %>
    <tr>
      <% name = login.service_provider.try(:name) || login.service_provider.to_s %>
      <td><%= link_to name, name %></td>
      <td><%= login.auth_type %></td>
      <td><%= login.created_at.to_formatted_s(:long) %></td>
    </tr>
    <%- end %>
  </tbody>
</table>

<%= will_paginate @logins %>
