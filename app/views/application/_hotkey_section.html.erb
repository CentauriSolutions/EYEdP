<div class="card col-6">
  <div class="card-header">
    <h5 class="card-title">
      name
    </h5>
  </div>
  <div class="card-body">
    <ul class="list-group">
      <%- hotkeys.each do |key| %>
      <%= render partial: 'hotkey_doc', locals: { name: key[:name], hotkey: key[:hotkey] } %>
      <%- end %>
    </ul>
  </div>
</div>
